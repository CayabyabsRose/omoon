{
  "name": "quswan.net",
  "title": "Ocampo's Moon |",
  "description": "The Socially Aware Magic Swordsman's independent blog site made by Metalsmith",
  "version": "0.5.0",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "metalsmith": "^2.3.0",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-browser-sync": "^1.1.1",
    "metalsmith-collections": "^0.9.0",
    "metalsmith-drafts": "0.0.1",
    "metalsmith-html-minifier": "^2.2.0",
    "metalsmith-layouts": "^1.8.1",
    "metalsmith-mapsite": "^1.0.4",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-permalinks": "^0.5.0",
    "metalsmith-publish": "^0.1.5",
    "metalsmith-pug": "^2.0.0",
    "metalsmith-word-count": "0.0.4"
  },
  "plugins": {
    "metalsmith-layouts": {
      "engine": "pug",
      "directory": "_layouts",
      "default": "layout.pug"
    },
    "metalsmith-pug": {
      "pretty": false,
      "useMetadata": true
    }
  },
  "repositories": {
    "type": "git",
    "url": "git+ssh://git@github.com/CayabyabsRose/quswan.git"
  },
  "scripts": {
    "build": "node index",
    "start": "npm install && npm run build && npm run deploy",
    "deploy": "deploy.sh"
  }
}
