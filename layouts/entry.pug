//- entry.pug - Layout for entries/posts.
extends layout.pug

block content
  article
    div.entry-wrapper
      header.entry-header
        h2.entry-title= title
        h3.entry-meta= date
          
      div.entry-content
        != contents

block footer
  footer.web-footer
    div
      p.home
        a.home-link(href='/') Home
